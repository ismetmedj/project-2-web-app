<h1>Profil of {{user.username}}</h1>
<div id="main">
  <button id="create">Create a Time Table</button>
</div>
<template id="createTT">
  <form id="createTimeTable" action="/profil" method="post">
    <label for="title">Title: </label>
    <input type="text" name="title" id="title" />
    {{! <label for="Participants"></label> }}
    <button>Create</button>
  </form>
</template>
<ul>
    {{#each allTT}}
    <li><a href="/timetable/{{this._id}}">{{this.title}}</a><button id="delete">Delete</button></li>
    {{/each}}
</ul>